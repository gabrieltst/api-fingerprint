{
  "name": "api-fingerprint",
  "version": "1.0.0",
  "description": "API para cadastro de biometria fingerprint com autenticação JWT",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test mocha --reporter mochawesome --reporter-options reportDir=reports,reportFilename=test-report",
    "test:simple": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --reporter mochawesome --reporter-options reportDir=reports,reportFilename=test-report",
    "test:watch": "cross-env NODE_ENV=test mocha --watch --reporter spec",
    "report": "node scripts/generate-report.js",
    "test:full": "npm run test && npm run report"
  },
  "keywords": [
    "fingerprint",
    "biometria",
    "api",
    "jwt",
    "express"
  ],
  "author": "Mentoria 2.0 Testes de Software",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3",
    "chai": "^4.3.7",
    "sinon": "^15.1.0",
    "cross-env": "^7.0.3"
  }
} 